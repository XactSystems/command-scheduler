{
    "name": "xactsystems/command-scheduler",
    "description": "A Symfony command scheduler.",
    "version": "3.4.22",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Foulds",
            "email": "ianfoulds@x-act.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xact\\CommandScheduler\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\CommandScheduler\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "doctrine/doctrine-bundle": "^1.12",
        "doctrine/orm": "^2.6",
        "symfony/asset": "^3.4",
        "symfony/form": "^3.4",
        "symfony/process": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/yaml": "*",
        "dragonmantank/cron-expression": "^3.0",
        "sensio/framework-extra-bundle": "~3.0|~5.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/phpunit-bridge": "^6.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
