{
    "name": "local/command-scheduler",
    "description": "A Symfony command scheduler.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Foulds",
            "email": "ianfoulds@x-act.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xact\\CommandScheduler\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\CommandScheduler\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.3",
        "doctrine/doctrine-bundle": "~2.1",
        "doctrine/orm": "~2.7",
        "dragonmantank/cron-expression": "~1.1",
        "sensio/framework-extra-bundle": "~5.5",
        "symfony/asset": "^4.0|^5.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/test-pack": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*"
    }
}
