{
    "name": "xactsystems/command-scheduler",
    "description": "A Symfony command scheduler.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Foulds",
            "email": "ianfoulds@x-act.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xact\\CommandScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\CommandScheduler\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^3.2",
        "symfony/asset": "^6.4||^7.1",
        "symfony/form": "^6.4||^7.1",
        "symfony/process": "^6.4||^7.1",
        "symfony/twig-bundle": "^6.4||^7.1",
        "symfony/yaml": "*",
        "dragonmantank/cron-expression": "^3.3",
        "xactsystems/cast": "^1.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/browser-kit": "^6.4||^7.1",
        "symfony/css-selector": "^6.4||^7.1",
        "symfony/phpunit-bridge": "^7.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
